@model IEnumerable<QLThuVien.Models.DOCGIA>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
</p>
<table class="table">
    <tr>
        <th>
            Tên:
        </th>
        <th>
            Địa chỉ:
        </th>
        <th>
            Điện thoại:
        </th>
        <th>
            Email:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenDG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DienThoai)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.MaDG }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaDG })
        </td>
    </tr>
}

</table>
